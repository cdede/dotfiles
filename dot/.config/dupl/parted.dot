// dot -Tpng  parted.dot -o line1.png
digraph G {
  bgcolor ="#009900";

	subgraph cluster_0 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=cyan];
		a2 ;
                vgroot;
                vghome;
                vg3;
		label = "/dev/sdan";
	}

	subgraph cluster_2 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=yellow];
		boot;
		backup;
                livehd;
		root ;
                usr;
                var;
		swap;
		home;
		label = "name";
	}

	a2 -> boot;
	a2 -> ext3;
	a2 -> autofs;
        vgroot -> swap ;
        vgroot -> root ;
        root -> ext4;
        vgroot -> usr ;
        usr ->ext4;
        vgroot -> var ;
        var ->ext4;
        vghome -> home;
	home -> ext4;
        vg3 ->backup;
        vg3 ->livehd;
        livehd ->xfs;
        backup -> ext4;
        backup -> autofs;
}
