digraph G {
  bgcolor ="#009900";

	subgraph cluster_0 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=cyan];
		a2 -> a13->a15;
                vgroot;
                vghome;
		label = "/dev/sdan";
	}

	subgraph cluster_2 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=yellow];
		boot;
		backup;
                livehd;
		root ;
                usr;
                var;
		swap;
		home;
		label = "name";
	}

	a2 -> boot;
	a2 -> ext3;
	a2 -> autofs;
        vgroot -> swap ;
        vgroot -> root ;
        root -> ext4;
        vgroot -> usr ;
        usr ->ext4;
        vgroot -> var ;
        var ->ext4;
        a15 -> backup;
        a15 -> nilfs2;
        a15 -> autofs;
        vghome -> home;
	home -> ext4;
        a13 ->livehd;
        a13 ->jfs;
}
