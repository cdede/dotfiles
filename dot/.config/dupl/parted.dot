// dot -Tpng  parted.dot -o line1.png
//  default ext4
digraph G {
  bgcolor ="#009900";

	subgraph cluster_0 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=cyan];
		a2 ;
                vgroot;
                vghome;
		label = "/dev/sdan";
	}

	subgraph cluster_2 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=yellow];
		boot;
		backup;
                livehd;
		root ;
                usr;
                var;
		swap;
		home;
		label = "name";
	}

	a2 -> boot;
	a2 -> ext3;
	a2 -> autofs;
        vgroot -> swap ;
        vgroot -> root ;
        vgroot -> usr ;
        vgroot -> var ;
        vghome -> home;
        vgroot ->livehd;
        livehd ->jfs;
        backup -> cryptsetup;
        backup -> cloud;
}
