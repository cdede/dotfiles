// dot -Tpng  parted.dot -o line1.png
digraph G {
  bgcolor ="#009900";

	subgraph cluster_0 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=cyan];
		a2 -> a3 ->a4-> a5->a6->a7;
		label = "/dev/sdan";
	}

	subgraph cluster_1 {
		node [style=filled];
		btrfs;
                ext2;
                xfs;
                autofs;
                nilfs2;
		label = "type";
		color=blue
	}
	subgraph cluster_2 {
		style=filled;
		color=lightgrey;
		node [style=filled,color=yellow];
		boot;
		backup;
                livehd;
		root ;
                pkg;  //  /var/cache/pacman/pkg
		swap;
		home;
		label = "name";
	}

	a2 -> boot;
	a2 -> ext2;
	a2 -> autofs;
        a3 -> swap ;
        a4 -> backup;
        a4 -> nilfs2;
        a4 -> autofs;
        a5 -> root ;
        a5 -> pkg ;
	a5 -> btrfs;
	a6 -> btrfs;
        a6 -> home;
        a7 ->livehd;
        a7 ->xfs;
}
